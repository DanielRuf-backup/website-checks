{
  "name": "website-checks",
  "version": "1.15.0",
  "main": "index.js",
  "description": "checks websites with multiple services",
  "scripts": {
    "test": "npm-run-all test:*",
    "test:crtsh": "node index.js example.com --crtsh",
    "test:cryptcheck": "node index.js example.com --cryptcheck",
    "test:hstspreload": "node index.js example.com --hstspreload",
    "test:httpobservatory": "node index.js example.com --httpobservatory",
    "test:lighthouse": "node index.js example.com --lighthouse",
    "test:psi": "node index.js example.com --psi",
    "test:securityheaders": "node index.js example.com --securityheaders",
    "test:ssldecoder": "node index.js example.com --ssldecoder",
    "test:ssldecoder-fast": "node index.js example.com --ssldecoder-fast",
    "test:ssllabs": "node index.js example.com --ssllabs",
    "test:webbkoll": "node index.js example.com --webbkoll",
    "test:webhint": "node index.js example.com --webhint"
  },
  "bin": {
    "website-checks": "./bin/website-checks.js"
  },
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DanielRuf/website-checks.git"
  },
  "keywords": [
    "certificate",
    "cryptcheck",
    "hsts",
    "hsts preload",
    "http observatory",
    "lighthouse",
    "pagespeed insights",
    "psi",
    "security headers",
    "sonarwhal",
    "ssl decoder",
    "ssllabs"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DanielRuf/website-checks/issues"
  },
  "homepage": "https://github.com/DanielRuf/website-checks#readme",
  "dependencies": {
    "kleur": "3.0.2",
    "puppeteer": "1.13.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
